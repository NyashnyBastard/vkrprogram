<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".cheklistActivity">


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/getBack"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_margin="40dp"
            android:backgroundTint="@color/material_on_primary_disabled"
            android:foregroundGravity="left"
            android:onClick="getBackTo"
            android:orientation="vertical"
            app:srcCompat="@drawable/ic_baseline_keyboard_backspace_24"
            tools:ignore="SpeakableTextPresentCheck" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="200dp"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.chip.Chip
            android:id="@+id/chip5"
            android:layout_width="300dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:backgroundTint="@color/teal_700"
            android:fontFamily="@font/kadwa"
            android:text="ЧЕК-ЛИСТ УТРО"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip6"
            android:layout_width="300dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:backgroundTint="@color/teal_700"
            android:fontFamily="@font/kadwa"
            android:text="ЧЕК-ЛИСТ ОБЕД"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip4"
            android:layout_width="300dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:backgroundTint="@color/teal_700"
            android:fontFamily="@font/kadwa"
            android:text="ЧЕК-ЛИСТ ВЕЧЕР"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chip7"
            android:layout_width="300dp"
            android:layout_height="80dp"
            android:layout_gravity="center"
            android:backgroundTint="@color/teal_700"
            android:fontFamily="@font/kadwa"
            android:text="КОНТРОЛЬ НАЧИНОК"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            android:typeface="normal" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>